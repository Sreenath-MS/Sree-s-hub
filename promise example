<html>
<head>
<body>
<input type="button" id="get" onclick="promiseToCleanTheRoom()" value="Get Details">
<script>
function promiseToCleanTheRoom() {
    return new Promise((resolve,reject)=>{
        let isClean=true;
        if(isClean){
            resolve('Clean');
        }
        else{
            reject('Not Clean');
        }
        });
        promiseToCleanTheRoom.then(function(fromResolve){
        console.log('The room is'+fromResolve);
        }).catch(function(fromReject){
            console.log('The room is'+fromReject);
        })
}
</script>
</body>
</head>
</html>
